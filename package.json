{
  "$schema": "http://json.schemastore.org/package",
  "name": "ng-repo",
  "version": "1.0.0",
  "description": "angular study docs",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --optimization=true --commonChunk=true --extractCss=true",
    "server": "nodemon server.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint-ts": "ng lint --typeCheck",
    "lint-ts:fix": "ng lint --typeCheck --fix",
    "lint-style": "stylelint src/**/*.less --syntax less",
    "lint-style:fix": "stylelint src/**/*.less --syntax less --fix",
    "lint-md": "lint-md .",
    "lint-md:fix": "lint-md --fix .",
    "e2e": "ng e2e",
    "test": "ng test",
    "eclint": "bash -c 'env eclint check $(git ls-files)'",
    "eclint:fix": "bash -c 'env eclint fix $(git ls-files)'"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --list-different"
    ],
    "*.md": [
      "lint-md --fix"
    ],
    "*.{css,less}": [
      "stylelint"
    ],
    "*.ts": [
      "tslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wzh327083434/ng-repo.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "vuepress"
  ],
  "author": "wuzhihao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wzh327083434/ng-repo/issues"
  },
  "homepage": "https://github.com/wzh327083434/ng-repo#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "core-js": "~2.6.9",
    "eclint": "^2.8.1",
    "exec-extra": "2.4.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-md-cli": "^0.1.2",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "protractor": "~5.4.3",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "~8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.7.5",
    "vuepress": "^1.3.0"
  },
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@uirouter/angular": "^6.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  }
}
